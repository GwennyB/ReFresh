@page
@model ReFreshMVC.Pages.Member.IndexModel
@using ReFreshMVC.Models
@{
    Layout = "../../Views/Shared/_Layout";
}

<div class="container">
    <div class="row">
        <div class="col m2"></div>
        <div class="col m8"><i class="large material-icons">person</i></div>
        <div class="col m2"></div>
    </div>
    <div classs="row">
        <form class="col s12">
            <div class="row">
                <div class="input-field col s6">
                    <input placeholder="Placeholder" id="first_name" type="text" class="validate">
                    <label asp-for="@Model.FirstName">First Name</label>
                </div>
                <div class="input-field col s6">
                    <input id="last_name" type="text" class="validate">
                    <label asp-for="@Model.LastName">Last Name</label>
                </div>
            </div>
        </form>

    </div>
</div>

@* Containier top image *@
<i class="material-icons">add</i>
